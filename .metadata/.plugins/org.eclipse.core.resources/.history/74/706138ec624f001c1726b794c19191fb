spring.application.name=Eureka-with-api-gateway
server.port=9100
eureka.instance.preferIpAddress=true
eureka.client.registerWithEureka=true
eureka.client.fetchRegistry=true
eureka.client.serviceUrl.defaultZone=${EUREKA_URI:http://localhost:8761/eureka}
zuul.routes.movie-info-service.url=http://localhost:8082/movies/
zuul.routes.ratings-data-service.url=http://localhost:8085/ratingsdata/user/
zuul.routes.movie-catalog-services.url=http://localhost:9000/catalog/
