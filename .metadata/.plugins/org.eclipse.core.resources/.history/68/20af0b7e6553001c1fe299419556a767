package com.tell.repository;

import java.util.Optional;

import org.springframework.data.mongodb.repository.MongoRepository;
import org.springframework.stereotype.Repository;

import com.tell.model.CropDetails;
@Repository
public interface CropDetailsRepository extends MongoRepository<CropDetails,String>{

	public Optional<CropDetails> findByCropName(String cropName);

	public Optional<CropDetails> findByCropType(String cropType);

	public void updateCropDetails(int id, CropDetails cropDetails);

	public CropDetails createCropDetails(CropDetails cropDetails);

	
	

}
