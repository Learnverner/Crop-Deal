package com.tell.model;

import java.util.Objects;

public class Items {
private String cropName;
private double cropPrice;
private int cropQuantity;


public Items() {
	super();
	
}

public Items(String cropName, double price, int quantity) {
	super();
	this.cropName = cropName;
	this.cropPrice = price;
	this.cropQuantity = quantity;
}

public String getCropName() {
	return cropName;
}
public void setCropName(String cropName) {
	this.cropName = cropName;
}
public double getPrice() {
	return cropPrice;
}
public void setPrice(double price) {
	this.cropPrice = price;
}
public int getQuantity() {
	return cropQuantity;
}
public void setQuantity(int quantity) {
	this.cropQuantity = quantity;
}
@Override
public int hashCode() {
	return Objects.hash(cropName, cropPrice, cropQuantity);
}
@Override
public boolean equals(Object obj) {
	if (this == obj)
		return true;
	if (obj == null)
		return false;
	if (getClass() != obj.getClass())
		return false;
	Items other = (Items) obj;
	return Objects.equals(cropName, other.cropName)
			&& Double.doubleToLongBits(cropPrice) == Double.doubleToLongBits(other.cropPrice) && cropQuantity == other.cropQuantity;
}

}
